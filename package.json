    "version": "conventional-changelog -p karma -i CHANGELOG.md -s -r 0 && git add ."
  },
  "author": "Philip Stapelfeldt <me@ph1p.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ph1p/fyfill.git"
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "axios": "^0.22.0",
    "buffer": "^6.0.3",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-sync": "^3.0.0",
    "preact": "^10.5.14",
    "react-popper": "^2.2.5",
    "react-query": "^3.25.1",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.36.0",
